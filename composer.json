{
	"name": "j7-dev/tgm-plugin-activation-forked",
	"description": "This is a library forked from tgmpa/tgm-plugin-activation and add some custom config and features",
	"version": "1.2.5",
	"type": "library",
	"homepage": "http://tgmpluginactivation.com",
	"keywords": [
		"wordpress",
		"plugins",
		"theme",
		"library",
		"activation"
	],
	"license": "GPL-2.0+",
	"autoload": {
		"files": [
			"index.php"
		]
	},
	"authors": [
		{
			"name": "J7",
			"email": "j7.dev.gg@gmail.com",
			"homepage": "https://github.com/j7-dev",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/j7-dev/tgm-plugin-activation-forked/issues",
		"source": "https://github.com/j7-dev/tgm-plugin-activation-forked"
	},
	"require": {
		"php": ">=5.2"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "@stable",
		"wp-coding-standards/wpcs": "@stable",
		"dealerdirect/phpcodesniffer-composer-installer": "@stable",
		"phpcompatibility/php-compatibility": "@stable",
		"phpstan/phpstan": "@stable",
		"php-stubs/woocommerce-stubs": "@stable",
		"php-stubs/wordpress-stubs": "@stable",
		"phpstan/extension-installer": "@stable"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
